{
    "name": "sg-network",
    "x-type": "hlfv1",
    "version": "1.0.0",
    "peers": {
        "peer0.sgorg.sg.com": {
            "url": "grpcs://localhost:7051",
            "eventUrl": "grpcs://localhost:7053",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.sgorg.sg.com"
            },
            "tlsCACerts": {
                "pem": "INSERT_SGORG_CA_CERT"
            }
        },
        "peer1.sgorg1.sg.com": {
            "url": "grpcs://localhost:8051",
            "eventUrl": "grpcs://localhost:8053",
            "grpcOptions": {
                "ssl-target-name-override": "peer1.sgorg.sg.com"
            },
            "tlsCACerts": {
                "pem": "INSERT_SGORG_CA_CERT"
            }
        },
    },
    "certificateAuthorities": {
        "ca.sgorg.example.com": {
            "url": "https://localhost:7054",
            "caName": "ca-sgorg",
            "httpOptions": {
                "verify": false
            }
        }
    },
    "orderers": {
        "orderer.sg.com": {
            "url": "grpcs://localhost:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.sg.com"
            },
            "tlsCACerts": {
                "pem": "INSERT_ORDERER_CA_CERT"
            }
        }
    },
    "organizations": {
        "SGOrg": {
            "mspid": "SGOrgMSP",
            "peers": [
                "peer0.sgorg.sg.com",
                "peer1.sgorg.sg.com"
            ],
            "certificateAuthorities": [
                "ca.sgorg.sg.com"
            ]
        }
    },
    "channels": {
        "sgchannel": {
            "orderers": [
                "orderer.sg.com"
            ],
            "peers": {
                "peer0.sgorg.sg.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "eventSource": true
                },
                "peer1.sgorg.sg.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "eventSource": true
                }
            }
        }
    },
    "client": {
        "organization": "SGOrg",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300",
                    "eventHub": "300",
                    "eventReg": "300"
                },
                "orderer": "300"
            }
        }
    }
}
